# NC CA rights (cacl) - OpenSSL 0.9.7 Compliant with GT3.2
 access_id_CA      X509         '/C=US/O=UTAustin/OU=TACC/CN=TACC Certification Authority/UID=caman'
 pos_rights        globus        CA:sign
 cond_subjects     globus       "*"

# Previous OpenSSL Version  - Compliant with GT3.0.2
 access_id_CA      X509         '/C=US/O=UTAustin/OU=TACC/CN=TACC Certification Authority/USERID=caman'
 pos_rights        globus        CA:sign
 cond_subjects     globus       "*"

